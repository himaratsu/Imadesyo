{
	"name": "今でしょ！",
	"version": "1.0",
	"manifest_version": 2,
	"description": "あの「今でしょ」をfacebook上で楽しめます。",
	"icons": { "48": "icon.png" },
	"permissions": [ "tabs", "http://www.facebook.com/", "https://www.facebook.com/"],
	"content_scripts": [{
      "matches": ["http://www.facebook.com/*", "https://www.facebook.com/*"],
	  "js": ["jquery-1.8.2.min.js", "change_imadesyo.js", "like_imadesyo.js", "main.js"],
	  "css": ["style.css"]
    }],
	"web_accessible_resources": [
	  "images/1.jpeg",
	  "images/2.jpg",
	  "images/3.jpg",
	  "images/4.png",
	  "images/5.jpg",
	  "images/6.jpg",
	  "images/7.jpg"
	]
}
